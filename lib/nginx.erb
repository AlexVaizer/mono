server {
    ssl_certificate <%="#{@env_values['mono_ssl']}/cert.pem;"%>
    ssl_certificate_key <%="#{@env_values['mono_ssl']}/privkey.pem;%>
    ssl on;
    add_header Strict-Transport-Security "max-age=31536000; includeSubdomains;";
    listen <%="#{@env_values['port']};"%> ssl;
    server_name <%="#{@env_values['domain']};"%>

    location / {
        proxy_pass http://localhost:8080;
    }
}